{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Compile (Debug)",
			"type": "actionscript",
			"debug": true,
			"group": "build",
			"problemMatcher": [],
			"dependsOrder": "sequence",
			"dependsOn": "Clean"
		},
		{
			"label": "Clean",
			"type": "actionscript",
			"clean": true,
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Compile (Release)",
			"type": "actionscript",
			"debug": false,
			"group": "build",
			"problemMatcher": [],
			"dependsOrder": "sequence",
			"dependsOn": "Clean"
		},
		{
			"label": "Build & Run: BYMR | Debug",
			"type": "shell",
			"command": "${workspaceFolder}/archived/flashplayers/windows/flashplayer_debug.exe",
			"args": [
				"",
				"bin/bymr-stable.swf"
			],
			"linux": {
				"args": [
					"${workspaceFolder}/bin/bymr-stable.swf"
				],
				"command": "${workspaceFolder}/archived/flashplayers/linux/flashplayerdebugger"
			},
			"osx": {
				"args": [
					"${workspaceFolder}/bin/bymr-stable.swf"
				],
				"command": "${workspaceFolder}/archived/flashplayers/FlashPlayerDebugger.app/Contents/MacOS/Flash Player Debugger",
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOrder": "sequence",
			"dependsOn": "Compile (Debug)"
		},
		{
			"label": "Build & Run: BYMR | Release",
			"type": "shell",
			"command": "${workspaceFolder}/archived/flashplayers/windows/flashplayer.exe",
			"args": [
				"",
				"bin/bymr-stable.swf"
			],
			"linux": {
				"args": [
					"${workspaceFolder}/bin/bymr-stable.swf"
				],
				"command": "${workspaceFolder}/archived/flashplayers/linux/flashplayerdebugger"
			},
			"osx": {
				"args": [
					"${workspaceFolder}/bin/bymr-stable.swf"
				],
				"command": "${workspaceFolder}/archived/flashplayers/FlashPlayerDebugger.app/Contents/MacOS/Flash Player Debugger",
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOrder": "sequence",
			"dependsOn": "Compile (Release)"
		}
	]
}