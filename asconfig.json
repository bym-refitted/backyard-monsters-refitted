{
	"config": "flex",
	"type": "app",
	"mainClass": "GAME",
	"compilerOptions": {
		"output": "bin/bymr-debug.swf",
		"target-player": "11.0",
		"swf-version": 13,
		"default-frame-rate": 40,
		"default-background-color": "#FFFFFF",
		"default-size": {
			"width": 760,
			"height": 670
		},
		"optimize": false,
		"strict": true,
		"warnings": true,
		"use-network": true,
		"advanced-telemetry": true,
		"static-link-runtime-shared-libraries": true,
		"source-path": [
			"client/scripts"
		],
		"library-path": [
			"./playerglobal.swc"
		],
		"define": [
			{
				"name": "CONFIG::SERVER_URL",
				"value": "'http://localhost:3001/'"
			},
			{
				"name": "CONFIG::CDN_URL",
				"value": "'http://localhost:3001/'"
			}
		]
	}
}
