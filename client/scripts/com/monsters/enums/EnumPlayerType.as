package com.monsters.enums
{
   /*
    * Enum representing Adobe Flash/AIR player types from Capabilities.playerType
    */
   public final class EnumPlayerType
   {

      /*
       * Adobe AIR desktop runtime
       */
      public static const DESKTOP:String = "Desktop";

      /*
       * Flash Player standalone application
       */
      public static const STANDALONE:String = "StandAlone";

      /*
       * Flash Player browser plugin
       */
      public static const PLUGIN:String = "PlugIn";

      /*
       * External Flash Player
       */
      public static const EXTERNAL:String = "External";
   }
}
